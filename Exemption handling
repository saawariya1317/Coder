#include<iostream>
using namespace std;

int main(){
    
    int a ,b ;
     cout<<"Enter two number\n";
     cin>>a>>b;
     
     
     try{
     if(b==0){
         throw 0;
     }
    cout<<"Division = "<<a/b<<endl;
     }
     
     
    catch(int x ){
        cout<<"Division Not Possible\n";
    }
    
    
    return 0;
}
