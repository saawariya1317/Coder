//sum
float volume(float a ,float b , float  c, int d){
    return a+b+c+d;
}
 
                        //   FUNCTION OVERLOADING

 // area of circle
float volume(int r){
    return (3.14 * r *r);
} 



//Volume of cylinder

float volume(int r, int h){
    return (3.14*r*r*h);
}


//volume of cuboid

float volume(int l , int b, int h ){
    return (l*b*h);
}

int main(){
    
    
cout<<"hello"<<endl;
cout<<volume(7)<<endl;
cout<<volume(3,4)<<endl;
cout<<volume(4,3,2)<<endl;
cout<<volume(1.3,2.4,3.7,4)<<endl;

return 0;
    
}
