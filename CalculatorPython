while(True):
    first = input("Enter first number : ")
    operator = input("Enter operator that you want to perform (+ , - , * , / , %): ")
    second = input("Enter second number : ")
    
    first = int(first)
    second = int(second)
    
    if operator == '+':
        print(first  + second)
    elif operator == '-':
        print(first  - second)
    elif operator == '*':
        print(first  * second)
    elif operator == '/':
        print(first  / second)
    elif operator == '%':
        print(first  % second)
    elif operator == '//':
        print(first  // second)
    else:
        print("Invalid operation")
        
    print('\n')

