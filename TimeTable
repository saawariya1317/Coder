#include<stdio.h>
void Goodmorning();     //Function Prototype
void Goodafternoon();   //Function Prototype
void Goodnight();       //Function Prototype


int main()
{
    printf("Initialing timetable function\n");
    Goodmorning();      //Function call
    Goodafternoon();    // "
    Goodnight();        // "
    
    
    return 0;
}


void Goodmorning()                      //Function definations
{
    printf("Goodmorning Gopal\n");
}

void Goodafternoon()
{
    printf("Goodafternoon Gopal\n");
}

void Goodnight()
{
    printf("Goodnight Gopal\n");
}


